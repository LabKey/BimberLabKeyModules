<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>WNPRC_Data</name>
    <description>WNPRC Clinical/Demographics Data</description>
    <transforms>
        <transform type="SimpleQueryTransformStep" id="demographics">
            <description>Copy to target</description>
            <source schemaName="mcc" queryName="aggregatedDemographics" >
                <sourceColumns>
                    <column>Id</column>
                    <column>date</column>
                    <column>gender</column>
                    <column>birth</column>
                    <column>death</column>
                    <column>species</column>
                    <column>status</column>
                    <column>mostRecentWeight</column>
                    <column>colony</column>
                    <column>objectid</column>
                </sourceColumns>
            </source>
            <destination schemaName="study" queryName="Demographics" targetOption="truncate" bulkLoad="true">
                <columnTransforms>
                    <column source="status" target="calculated_status"/>
                </columnTransforms>
                <alternateKeys>
                    <column name="objectid"/>
                </alternateKeys>
            </destination>
        </transform>
    </transforms>
    <schedule>
        <!--11PM-->
        <cron expression="0 0 23 * * ?"/>
    </schedule>
</etl>
