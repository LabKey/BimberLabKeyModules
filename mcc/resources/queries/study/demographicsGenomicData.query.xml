<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsGenomicData" tableDbType="NOT_IN_DB">
                <tableTitle>Genomic Data Summary</tableTitle>
                <description>Genomic Data Summary</description>
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="datatypes">
                        <columnTitle>Data Types</columnTitle>
                        <url>/query/executeQuery.view?
                            schemaName=study&amp;
                            query.queryName=genomicDatasets&amp;
                            query.Id~eq=${Id}
                        </url>
                    </column>
                    <column columnName="sra_accession">
                        <columnTitle>SRA Accession(s)</columnTitle>
                    </column>
                </columns>
                <titleColumn>datatypes</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
