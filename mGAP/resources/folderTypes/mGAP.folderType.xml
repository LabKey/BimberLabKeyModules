<folderType xmlns="http://labkey.org/data/xml/folderType">
    <name>mGAP</name>
    <description>The default folder layout for mGAP</description>
    <folderTabs>
        <folderTab>
            <name>overview</name>
            <caption>Overview</caption>
            <selectors>
                <selector>
                    <controller>mgap</controller>
                </selector>
            </selectors>
            <requiredWebParts>

            </requiredWebParts>
            <preferredWebParts>
                <webPart>
                    <name>mGAP Overview</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
        <folderTab>
            <name>variants</name>
            <caption>Variant Catalog</caption>
            <selectors>
                <selector>
                    <view>variants</view>
                </selector>
            </selectors>
            <requiredWebParts>
            </requiredWebParts>
            <preferredWebParts>
                <webPart>
                    <name>mGAP Variant Releases</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
        <folderTab>
            <name>datasets</name>
            <caption>Raw Data</caption>
            <selectors>
                <selector>
                    <view>data</view>
                </selector>
            </selectors>
            <requiredWebParts>
            </requiredWebParts>
            <preferredWebParts>
                <webPart>
                    <name>mGAP Raw Data</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
    </folderTabs>
    <modules>
        <moduleName>mGAP</moduleName>
        <moduleName>JBrowse</moduleName>
        <moduleName>SequenceAnalysis</moduleName>
        <moduleName>Laboratory</moduleName>
        <moduleName>Query</moduleName>
        <moduleName>Pipeline</moduleName>
        <moduleName>Core</moduleName>
    </modules>
    <defaultModule>Core</defaultModule>
    <forceAssayUploadIntoWorkbooks>true</forceAssayUploadIntoWorkbooks>
    <folderIconPath>/sequenceanalysis/icons/dna_folder.png</folderIconPath>
</folderType>