<customView xmlns="http://labkey.org/data/xml/queryCustomView" canOverride="true">
   <columns>
       <column name="analysisId" />
       <column name="sampleName" />
       <column name="cDNA/sortId/sampleId/subjectId" />
       <column name="cDNA/sortId/sampleId/sampledate" />
       <column name="cDNA/sortId/sampleId/stim" />
       <column name="cDNA/sortId/population" />
       <column name="calculatedPopulation" />
       <column name="locus" />
       <column name="vHit" />
       <column name="dHit"/>
       <column name="jHit"/>
       <column name="cHit"/>
       <column name="CDR3"/>
       <column name="cloneNames"/>
       <column name="count"/>
       <column name="fraction"/>
       <column name="cloneId"/>
       <column name="vGene" />
       <column name="dGene"/>
       <column name="jGene"/>

       <column name="comment" />
       <column name="Run/assayName" />
       <column name="workbook" />
       <column name="pipelineRunId/JobId" />
       <column name="pipelineRunId">
           <properties>
               <property name="columnTitle" value="Run"/>
           </properties>
       </column>
       <column name="disabled"/>
   </columns>
   <sorts>
       <sort column="cDNA/sortId/population"/>
       <sort column="locus"/>
       <sort column="count" descending="true"/>
   </sorts>
</customView>